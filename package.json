{
  "name": "@alaskaairux/orion-design-tokens",
  "version": "1.1.0",
  "description": "Alaska Air Orion Design System token repository",
  "homepage": "https://github.com/AlaskaAirlines/OrionDesignTokens",
  "author": "Alaska Airlines Product design and research",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlaskaAirlines/OrionDesignTokens"
  },
  "main": "src/",
  "license": "Apache-2.0",
  "dependencies": {
    "style-dictionary": "^2.7.0"
  },
  "devDependencies": {
    "auto-changelog": "^1.12.0",
    "concat": "^1.0.3",
    "copyfiles": "^2.1.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-jsonlint": "^1.2.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sequence": "^1.0.0",
    "postcss-custom-properties": "^8.0.8",
    "style-dictionary": "^2.7.0",
    "versiony-cli": "^1.3.0"
  },
  "scripts": {
    "buildTokens": "node scripts/styleDictionary.js",
    "changelog": "auto-changelog --template ./scripts/template.hbs --output ./dist/CHANGELOG.md -p -b 5",
    "copyJson": "copyfiles -u 1 -V './src.package.json' './src/**/*.json' ./dist",
    "copyReadme": "copyfiles -f -V './src/npmREADME.md' ./dist",
    "concat": "concat -o ./dist/README.md ./dist/npmREADME.md ./dist/CHANGELOG.md",
    "set-patch": "versiony ./package.json --patch"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "alaska airlines",
    "style dictionary",
    "orion design system",
    "css",
    "properties",
    "tokens",
    "sass",
    "ios",
    "android"
