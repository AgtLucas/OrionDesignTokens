{
  "source": ["src/properties/**/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "build/scssTokens/",
      "files": [{
        "destination": "_TokenVariables.scss",
        "format": "scss/variables"
      }]
    },
    "css": {
      "transformGroup": "css",
      "buildPath": "build/cssTokens/",
      "files": [{
        "destination": "_TokenVariables.css",
        "format": "css/variables"
      }]
    }
  }
}
